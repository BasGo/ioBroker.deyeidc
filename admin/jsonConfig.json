{
	"i18n": true,
	"type": "tabs",
	"items": {
		"_configuration": {
			"type": "panel",
			"label": "Settings",
			"items": {
				"_headerSettings": {
					"newLine": true,
					"type": "header",
					"text": "Settings",
					"size": 2
				},
				"ipaddress": {
					"type": "text",
					"sm": 12,
					"md": 6,
					"lg": 6,
					"label": "IP-Address from Inverter"
				},
				"port": {
					"type": "number",
					"sm": 12,
					"md": 6,
					"lg": 6,
					"label": "Port ( usually 8899 )"
				},
				"logger": {
					"type": "number",
					"sm": 12,
					"md": 6,
					"lg": 6,
					"label": "Serial Number Data Logger",
					"newLine": true
				},
				"pollInterval": {
					"type": "number",
					"sm": 12,
					"md": 6,
					"lg": 6,
					"label": "Polling rate"
				}
			}
		},
		"_registers": {
			"type": "panel",
			"label": "Register",
