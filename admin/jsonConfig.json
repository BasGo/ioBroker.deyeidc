					"text": "Register Sections",
					"size": 2
				},
				"registers": {
					"type": "table",
					"sm": 12,
					"md": 12,
					"lg": 12,
					"items": [
						{
							"type": "number",
							"attr": "registerStart",
							"width": "35%",
							"title": "Register Start",
							"default": ""
						},
						{
							"type": "number",
							"attr": "registerEnd",
							"width": "35%",
							"title": "Register End",
							"default": ""
						}
					]
				},
				"_help1": {
					"newLine": true,
					"type": "staticText",
					"text": "table register help",
					"sm": 12,
					"md": 4,
					"lg": 4
				}
			}
		},
