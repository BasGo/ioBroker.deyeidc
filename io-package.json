  "native": {
    "ipaddress": "",
    "port": 8899,
    "logger": null,
    "pollInterval": 60,
    "registers": [
      {
        "registerStart": 59,
        "registerEnd": 90
      }
    ],
    "coils": [
      {
        "register": 86,
        "rules": 3,
        "key": "Apo_t1",
        "name": "Total AC Output Power (Active)",
        "unit": "W",
        "factor": 1
      }
    ],
    "computes": [
      {
        "value1": "DV1",
        "value2": "DC1",
        "key": "DP1",
        "name": "DC Power PV1",
        "unit": "W",
        "factor": 2
      }
    ]
  },
